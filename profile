# shell prompt
PS1='[\u@\h \w]\#'

export PS1

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:lib:/usr/lib:/system/lib

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/system/vendor/lib

export PATH=/sbin:/bin:/usr/sbin:/usr/bin:/usr/local/bin:/system/bin

export PATH=$PATH:/var/log/jdk8.0.322-linux_aarch32sf/bin

export PATH=$PATH:/system/vendor/bin
